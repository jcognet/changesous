{% extends '@Commun/base.html.twig' %}
    {% block header %}
        Page de contact
    {% endblock %}
{% block body %}
    <div class="col-xs-12">
        {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.sujet) }}
        {{ form_row(form.corps) }}
        <div class="g-recaptcha" data-sitekey="{{ recaptcha_public_key }}"></div>
        {{ form_row(form.save) }}
        {{ form_end (form)}}
    </div>
{% endblock %}